<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal 1" serialNumber="(embedded)" parentModuleAddress="1">
        <LynxModule name="Control Hub" port="173">
            <ContinuousRotationServo
                name="CRSOne"
                port="0" />
            <ContinuousRotationServo
                name="CRSTwo"
                port="1" />
            <LynxColorSensor
                name="colorSensor"
                bus="3"
                port="1" />
        </LynxModule>
    </LynxUsbDevice>
</Robot>



